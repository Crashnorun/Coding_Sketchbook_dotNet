<Window x:Class="WPF_Slider.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Slider"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="21" Margin="40,200,0,0" Stroke="Black" VerticalAlignment="Top" Width="720"/>
        <Ellipse Fill="#FF3CAFF0" HorizontalAlignment="Left" Height="39" Margin="0,200,0,0" VerticalAlignment="Top" Width="39" MouseDown="Ellipse_MouseDown" MouseUp="Ellipse_MouseUp" MouseLeave="Ellipse_MouseLeave"/>
        <local:MySlider HorizontalAlignment="Center" VerticalAlignment="Center" Margin="10,10,10,363" Width="772" Height="30"></local:MySlider>
    </Grid>
</Window>
